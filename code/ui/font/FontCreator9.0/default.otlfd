feature Fractions frac {
  lookup SlashToFraction;
  lookup StandardFractions;
  lookup NumbersToNumerators;
  lookup FractionContext;
}

group @Denominators [fraction zero.dnom one.dnom two.dnom three.dnom four.dnom five.dnom six.dnom seven.dnom eight.dnom nine.dnom];
group @Numerators [zero.sups one.sups two.sups three.sups four.sups five.sups six.sups seven.sups eight.sups nine.sups];

lookup FractionContext {
  context (@Denominators) @Numerators;
  sub 0 NumeratorsToDenominators;
}

lookup StandardFractions { 
  # When adding a standard fraction here, make sure you also add it to the @Denominators group!
  sub one fraction two -> onehalf;
